{
    "name": "xandco/whoisparser",
    "description": "Whois Parser, parses and formats raw whois query data.",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Batres Santellano",
            "email": "miguel@batres.co",
            "homepage": "https://batres.co"
        }
    ],
    "homepage": "https://github.com/xandco/whoisparser",
    "keywords": ["Laravel", "WhoisParser"],
    "require": {
        "illuminate/support": "~5|~6|~7"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "xandco\\WhoisParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xandco\\WhoisParser\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xandco\\WhoisParser\\WhoisParserServiceProvider"
            ],
            "aliases": {
                "WhoisParser": "xandco\\WhoisParser\\Facades\\WhoisParser"
            }
        }
    }
}
